<%- model_class = Output -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:watt) %></th>
      <th><%= model_class.human_attribute_name(:voltage) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
        <th><%= model_class.human_attribute_name(:time) %></th>
        <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @outputs.each do |output| %>
      <tr>
        <td><%= link_to output.id, output_path(output) %></td>
        <td><%= output.watt %></td>
        <td><%= output.voltage %></td>
        <td><%= output.created_at.to_time %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_output_path(output), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      output_path(output),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_output_path,
            :class => 'btn btn-primary' %>
