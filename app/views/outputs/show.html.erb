<%- model_class = Output -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:watt) %>:</strong></dt>
  <dd><%= @output.watt %></dd>
  <dt><strong><%= model_class.human_attribute_name(:voltage) %>:</strong></dt>
  <dd><%= @output.voltage %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              outputs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_output_path(@output), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              output_path(@output),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
